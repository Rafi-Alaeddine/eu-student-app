MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here

PORT=8000
MOBILE_API_KEY=your_mobile_api_key_here

# Optional override for transport.rest base URL (default is public v6 transport.rest)
TRANSPORT_REST_BASE=https://v6.transport.rest

# CORS allowed origins (comma-separated). For development, include your emulator URL (e.g., http://10.0.2.2:8100) or leave blank to allow all.
ALLOWED_ORIGINS=http://localhost:3000,http://10.0.2.2:3000

# Cache TTL seconds for external requests (stops).
CACHE_TTL=300